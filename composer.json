{
    "name": "joe-nassar-tech/laravel-exponential-lockout",
    "description": "Laravel package for implementing exponential lockout on failed authentication attempts with configurable contexts",
    "version": "1.4.5",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "lockout",
        "exponential",
        "rate-limiting",
        "authentication",
        "security",
        "brute-force-protection",
        "middleware",
        "cache"
    ],
    "homepage": "https://github.com/joe-nassar-tech/laravel-exponential-lockout",
    "support": {
        "issues": "https://github.com/joe-nassar-tech/laravel-exponential-lockout/issues",
        "source": "https://github.com/joe-nassar-tech/laravel-exponential-lockout",
        "docs": "https://github.com/joe-nassar-tech/laravel-exponential-lockout/tree/main/docs"
    },
    "authors": [
        {
            "name": "Joe Nassar",
            "email": "joe.nassar.tech@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ExponentialLockout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExponentialLockout\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ExponentialLockout\\ExponentialLockoutServiceProvider"
            ],
            "aliases": {
                "Lockout": "ExponentialLockout\\Facades\\Lockout"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}